{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "fujiWhite": "#DCD7BA",
    "sumiInk0": "#16161D",
    "sumiInk4": "#54546D",
    "autumnGreen": "#76946A",
    "autumnRed": "#C34043",
    "roninYellow": "#FF9E3B",
    "waveAqua1": "#6A9589",
    "dragonBlue": "#658594",
    "springViolet2": "#9CABCA",
    "crystalBlue": "#7E9CD8",
    "springGreen": "#98BB6C",
    "boatYellow2": "#C0A36E",
    "sakuraPink": "#D27E99",
    "waveRed": "#E46876",
    "katanaGray": "#717C7C"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}p:boatYellow2{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:waveRed{{end}}",
            "{{if eq \"Full\" .State.String}}p:autumnGreen{{end}}"
          ],
          "properties": {
            "discharging_icon": "\udb81\ude8c",
            "charging_icon": "\udb85\udc0b",
            "charged_icon": "\ue22f"
          },
          "style": "plain",
          "template": "<p:sumiInk4>[</> {{ if ge .Percentage 69}}\udb84\udea3{{ else if and (ge .Percentage 40) (le .Percentage 68) }}\udb84\udea2{{ else if and (ge .Percentage 10) (le .Percentage 39) }}\udb84\udea1{{ else if le .Percentage 9 }}\udb80\udc8e{{end}} {{ .Percentage }}% \udb80\udddd {{ .Icon }} <p:sumiInk4>]</>",
          "type": "battery"
        },
        {
          "foreground": "p:sumiInk4",
          "template": "\u2800",
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "p:boatYellow2",
          "template": "<p:sumiInk4>[</> \udb80\udf5b {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GiB <p:sumiInk4>]</>",
          "properties": {
            "precision": 2
          },
          "style": "plain",
          "type": "sysinfo"
        },
        {
          "foreground": "p:sumiInk4",
          "template": "\u2800",
          "style": "plain",
          "type": "text"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:dragonBlue",
          "properties": {
            "time_format": "TimeOnly"
          },
          "template": "<p:sumiInk4>[</> \ue384 {{ .CurrentDate | date .Format }} <p:sumiInk4>]</>"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:sumiInk4",
          "style": "plain",
          "template": "\u250c",
          "type": "text"
        },
        {
          "background": "p:sumiInk4",
          "foreground": "p:crystalBlue",
          "style": "plain",
          "template": " {{.Icon}}{{if .WSL}} WSL{{end}}\u2800",
          "type": "os"
        },
        {
          "foreground": "p:sumiInk4",
          "template": "\u2500",
          "style": "plain",
          "type": "text"
        },
        {
          "background": "p:springViolet2",
          "foreground": "p:sumiInk0",
          "style": "plain",
          "template": " \uf120 {{.Name}}\u2800",
          "type": "shell"
        },
        {
          "foreground": "p:sumiInk4",
          "template": "\u2500",
          "style": "plain",
          "type": "text"
        },
        {
          "background": "p:waveAqua1",
          "foreground": "p:sumiInk0",
          "style": "plain",
          "template": " \uf011 {{ .HostName }}@{{ .UserName }}\u2800",
          "type": "session"
        },
        {
          "foreground": "p:sumiInk4",
          "template": "{{ if .HEAD }}\u2500{{ end }}",
          "style": "plain",
          "type": "git"
        },
        {
          "background": "p:autumnRed",
          "foreground": "p:fujiWhite",
          "properties": {
            "branch_icon": " \udb81\ude2c ",
            "fetch_status": true
          },
          "style": "plain",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf01f {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} | {{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}\u2800",
          "type": "git"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:sumiInk4",
          "style": "plain",
          "template": "\u2514",
          "type": "text"
        },
        {
          "foreground": "p:fujiWhite",
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_template": "<p:katanaGray> \udb83\udfdf </>",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "</><p:dragonBlue>[</> {{.Path}} ",
          "type": "path"
        },
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:springGreen",
          "properties": {
            "display_mode": "files",
            "fetch_package_manager": true,
            "extensions": [
              "*.js",
              "*.cjs",
              "*.mjs",
              "*.ts",
              "*.ts",
              "*.tsx",
              "package.json",
              "package-lock.json",
              "yarn.lock",
              "pnpm-lock.yaml"
            ]
          },
          "template": "(\ued0d {{ .Full }})\u2800"
        },
        {
          "type": "bun",
          "style": "plain",
          "foreground": "p:sakuraPink",
          "properties": {
            "display_mode": "files"
          },
          "template": "{{ if .Error }}{{ else }}(\ue76f {{ .Full }})\u2800{{ end }}"
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "p:waveRed",
          "properties": {
            "display_mode": "files"
          },
          "template": "{{ if .Error }} {{ else }}(\uedaf {{ .Full }})\u2800{{ end }}"
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:roninYellow",
          "properties": {
            "display_mode": "files",
            "extensions": [
              "*.py",
              "*.ipynb",
              "pyproject.toml",
              "venv.bak",
              ".venv/*",
              ".env/*"
            ]
          },
          "template": "{{ if .Error }}{{ else }}(\udb80\udf20 {{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }})\u2800{{ end }}"
        },
        {
          "type": "php",
          "style": "plain",
          "foreground": "p:springViolet2",
          "properties": {
            "display_mode": "files"
          },
          "template": "{{ if .Error }} {{ else }}(\udb80\udf1f {{ .Full }})\u2800{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "template": "<p:dragonBlue>]</>"
        },
        {
          "foreground": "p:katanaGray",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": ":",
          "type": "status"
        }
      ]
    }
  ],
  "console_title_template": "{{.Shell}} in {{.PWD}}",
  "final_space": true,
  "version": 3
}